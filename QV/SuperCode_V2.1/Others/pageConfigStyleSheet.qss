QLabel{
	text-align:left;
}

QLabel#lSendTestMail{
	color:#BA292A
}

QLabel#lPreviewToobar,#lPlaybackToobar{
	padding-left:25px;
	border-radius:2px;
	background:#232326
}

QLabel#infoPreviewToobar,#infoPlaybackToobar{
	font:13px;
	color:rgba(255,255,255,0.6)
}


QPushButton{
	background:#484D58;
	border-radius:2px;
}
QPushButton:hover{
	background:#6a6f7a;	
}
QPushButton:Pressed{
	background:#484d58;	
}

QPushButton[row],[row]:pressed,[row]:hover{
	background: transparent;
	width:40px;
}

QPushButton[col="3"]{
	background: url(res/default/image/config/path) no-repeat left;
}

QPushButton[col="3"]:hover{
	background: url(res/default/image/config/path_hover) no-repeat left;
}

QPushButton[col="3"]:pressed{
	background: url(res/default/image/config/path_pre) no-repeat left;
}

QPushButton[col="4"]{
	background: url(res/default/image/config/audio) no-repeat left;
}

QPushButton[col="4"]:hover{
	background: url(res/default/image/config/audio_pre) no-repeat left;
}

QPushButton[col="4"]:pressed{
	background: url(res/default/image/config/audio_pre) no-repeat left;
}

QPushButton[col="5"]{
	background: url(res/default/image/config/del) no-repeat left;
}
QPushButton[col="5"]:hover{
	background: url(res/default/image/config/del_pre) no-repeat left;
}
QPushButton[col="5"]:pressed{
	background: url(res/default/image/config/del_pre) no-repeat left;
}
QPushButton[col="5"]:disabled{
	background: none;
}

QPushButton#videopathPb,#imgpathPb{
	background:#0f1014; 
	image: url(res/default/image/config/path);
	border-top-right-radius:2px;
	border-bottom-right-radius:2px
}
QPushButton:hover#videopathPb,:hover#imgpathPb{
	image: url(res/default/image/config/path_hover);
	border:1px solid #1b6dc3;
}

QPushButton:pressed#videopathPb,:pressed#imgpathPb{
	image: url(res/default/image/config/path_pre);
}


QPushButton#addAudio{
	background:#1B6DC3; 
}
QPushButton:hover#addAudio{
	background:#298cf4;
}
QPushButton:pressed#addAudio{
	background:#1b6dc3;
}



QListWidget{
	font:14px;
	text-align:left;
	border-right:1px;
	border-style:solid;
    border-color:#0f1014;
	padding-top:4px;	
}


QListWidget::Item{
	color:rgba(255,255,255,0.6);
	padding:13px 0px 13px 26px;
	margin-left:4px;
}

QListWidget::Item:selected{
	background:#0f1014;
	color:#ffffff;
	font-weight:bold;
	border-style:solid;
	border-left-width:3px;
	border-left-color:#1b6dc3;
	padding-left:23px;	
}


QComboBox{
	border-top-left-radius:2px;
	border-bottom-left-radius:2px;
	background:#0f1014;
	margin-bottom:1px;
}

QComboBox:hover{
	border:1px solid #1b6dc3;
}

QComboBox::drop-down{
	subcontrol-origin: padding;
    subcontrol-position: top right;
	padding-right:8px;
    border-top-right-radius: 2px; 
    border-bottom-right-radius: 2px;
}

QComboBox::down-arrow{
	image:url(res/default/image/config/down_arrow);
}
QComboBox::down-arrow:hover{
	image:url(res/default/image/config/down_arrow_hover);
}
QComboBox::down-arrow:on {
    image:url(res/default/image/config/down_up);
}

QComboBox QAbstractItemView{ 
	border:1px solid #232326;
	margin-top:1px;
}
QComboBox QAbstractItemView::item{
	height: 30px;
	background:#0f1014;
	color:#ffffff;
}
QComboBox QAbstractItemView::item:hover{
	background:#232326;
}

QListView::item:hover{
	background:#0f1014;
}


QCheckBox::indicator{
	height:40px;
	width:40px;
	border-radius:2px;
}

QCheckBox::indicator:unchecked{  
    image:url(res/default/image/config/button_off);  
}  
  
QCheckBox::indicator:checked{  
    image:url(res/default/image/config/button_on);  
}  

QCheckBox::indicator:unchecke#PPause{
	
	background:#0F1014; 
    image:url(res/default/image/config/1);  
}  
  
QCheckBox::indicator:checked#PPause{  
	background:#1B6DC3;
}  


QCheckBox::indicator:unchecke#PLocate{
	
