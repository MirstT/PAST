QLineEdit{
    background:#0f1014;
	border-radius:2px;
}

QLineEdit:hover {
	border:1px solid #1b6dc3;
}




QSpinBox{
	border-radius:2px;
	background:#0f1014;
	border-color:#0f1014;
	text-align:center;
}

QSpinBox:hover{
	border-color:#1b6dc3;
}

QSpinBox::up-button,QSpinBox::down-button
{
	width:0px;
}



QTableView{
	color:#ffffff;	
	border:1px solid #232326;
	border-radius:2px;        
    background:#15161A; 
    alternate-background-color:#0F1014;
}

QTableView::item{
	color:#ffffff;
	background:transparent; 	
}
 

QHeaderView::section{	

	background:#232326;
	height:22px;
	border:0px;
	margin:0px;
	padding:6px 0px;
}

QScrollBar:vertical{  
	width:8px;
	background:#232326; 
	padding:38px 2px 3px 2px;  
}  
QScrollBar::handle:vertical{  
	border-radius:2px; 
	background:#484D58;
}  
QScrollBar::handle:vertical:hover{  
	background:#6A6F7A;
}
QScrollBar::add-page:vertical{
	background:#232326;
}
QScrollBar::sub-page:vertical{
	background:#232326;
}
QScrollBar::add-line:vertical,QScrollBar::sub-line:vertical{
	border:none;
	height:none;
}


QRadioButton::indicator::unchecked {
    image: url(res/default/image/config/rb_off);
}
 
QRadioButton::indicator:hover {
    image: url(res/default/image/config/rb_hov);
}
 
QRadioButton::indicator::checked {
    image: url(res/default/image/config/rb_pre);
}